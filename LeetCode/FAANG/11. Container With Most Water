/**
 * @param {number[]} height
 * @return {number}
 */
var maxArea = function(height) {
    let max = 0;
    let area = 1;

    for (let i = 0; i < height.length - 1; i++) {
        for (let j = i + 1; j < height.length; j++) {
            area = Math.min(height[i], height[j]) * (j - i);
            if (area > max) {
                max = area;
                area = 0;
            }
        }
    }
    return max;
};
